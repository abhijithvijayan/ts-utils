{
	"name": "@abhijithvijayan/ts-utils",
	"version": "0.11.0",
	"description": "Collection of JavaScript utility functions",
	"license": "MIT",
	"repository": "https://github.com/abhijithvijayan/ts-utils.git",
	"author": {
		"name": "abhijithvijayan",
		"email": "email@abhijithvijayan.in",
		"url": "https://abhijithvijayan.in"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"main": "lib/cjs/source/index.js",
	"module": "lib/esm/source/index.js",
	"types": "lib/cjs/source/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"watch": "tsc --module esnext --outDir lib/esm --watch",
		"build:cjs": "tsc --module commonjs --outDir lib/cjs",
		"build:esm": "tsc --module esnext --outDir lib/esm",
		"build": "rimraf lib && yarn build:cjs && yarn build:esm",
		"pack:list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"prepublishOnly": "yarn test && yarn build",
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint . --ext .ts"
		]
	},
	"keywords": [
		"npm",
		"module"
	],
	"dependencies": {},
	"devDependencies": {
		"@abhijithvijayan/eslint-config": "2.6.2",
		"@abhijithvijayan/eslint-config-airbnb": "^1.0.2",
		"@abhijithvijayan/tsconfig": "^1.3.0",
		"@babel/eslint-parser": "^7.13.14",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.0.2",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^11.0.0",
		"prettier": "^2.3.0",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.5.6",
		"typescript": "^4.2.4"
	}
}
